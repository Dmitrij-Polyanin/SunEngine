{
  "Server": {
    "FileServer": true,
    "UpdateConfigJsOnConfigChanges": false
  },
  "UrlPaths": {
    "Site": "http://localhost:5005",
    "Api": "http://localhost:5000",
    "UploadImages": "http://localhost:5005/upload-images",
    "Skins": "http://localhost:5005/skins",
    "PartialSkins": "http://localhost:5005/partial-skins",
    "CustomCss": "http://localhost:5000/custom.css",
    "CustomJavaScript": "http://localhost:5000/custom.js"
  },
  "Dirs": {
    "ApplicationRoot": "auto",
    "WwwRoot": "%app%/bin/wwwroot",
    "UploadImages": "%wwwroot%/upload-images",
    "MailTemplates": "%config%/MailTemplates",
    "Skins": "%wwwroot%/skins",
    "PartialSkins": "%wwwroot%/partial-skins",
    "Resources": "%app%/Resources"
  },
  "Kestrel": {
    "EndPoints": {
      "Http": {
        "Url": "http://localhost:5000"
      }
    }
  },
  "Cors": {
    "Origins": "http://localhost:5005",
    "AllowAnyOrigin": false,
    "AllowCredentials": true,
    "AllowAnyHeader": true,
    "AllowAnyMethod": true
  },
  "Security": {
    "CookieLongToken2SameSiteMode": "Strict" // One of the: "None" "Strict", "Lax"
  }
}