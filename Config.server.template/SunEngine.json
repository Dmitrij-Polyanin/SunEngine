{
  "Server": {
    "FileServer": true,
    "UpdateConfigJsOnConfigChanges": true
  },
  "UrlPaths": {
    "Site": "http://<domain>",
    "Api": "http://<domain>",
    "UploadImages": "http://<domain>/UploadImages",
    "Skins": "http://<domain>/statics/Skins",
    "PartialSkins": "http://<domain>/statics/PartialSkins"
  },
  "Dirs": {
    "ApplicationRoot": "auto",
    "WwwRoot": "%app%/wwwroot",
    "UploadImages": "%wwwroot%/UploadImages",
    "MailTemplates": "%config%/MailTemplates",
    "Skins": "%wwwroot%/statics/Skins",
    "PartialSkins": "%wwwroot%/statics/PartialSkins",
    "Resources": "%app%/Resources"
  },
  "Kestrel": {
    "EndPoints": {
      "Http": {
        "Url": "http://localhost:<port>"
      }
    }
  }
}
